{
  "name": "ai-chat-studio",
  "version": "3.0.0",
  "description": "Enterprise-grade AI chat platform with 95+ features: RAG, workflows, collaboration, analytics, multi-modal AI, and more. Full-stack React + TypeScript + Python FastAPI",
  "keywords": [
    "ai",
    "chat",
    "chatgpt",
    "claude",
    "gemini",
    "openai",
    "anthropic",
    "react",
    "typescript",
    "fastapi",
    "pwa",
    "mobile",
    "fullstack",
    "conversation",
    "llm",
    "ai-assistant",
    "chat-application",
    "chatbot",
    "machine-learning",
    "natural-language-processing"
  ],
  "author": "luckylwl <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/luckylwl/chat-studio.git"
  },
  "bugs": {
    "url": "https://github.com/luckylwl/chat-studio/issues"
  },
  "homepage": "https://github.com/luckylwl/chat-studio#readme",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "analyze": "vite-bundle-visualizer",
    "security-check": "node security-check.js",
    "setup-services": "node -e \"console.log(process.platform === 'win32' ? 'setup-services.bat' : 'bash setup-services.sh')\" && (node -p \"process.platform === 'win32' ? '.\\\\setup-services.bat' : 'bash setup-services.sh'\" | sh)",
    "prebuild": "npm run security-check"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/lodash-es": "^4.17.12",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/react-window": "^1.8.8",
    "@xenova/transformers": "^2.17.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "dompurify": "^3.0.5",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.16.4",
    "highlight.js": "^11.8.0",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.284.0",
    "mammoth": "^1.11.0",
    "marked": "^9.0.3",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^16.0.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^6.15.0",
    "react-syntax-highlighter": "^15.6.6",
    "react-window": "^2.1.2",
    "tailwind-merge": "^1.14.0",
    "xlsx": "^0.18.5",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/dompurify": "^3.0.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.6"
  }
}
